<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout
                    pattern="[%d{HH:mm:ss} %highlight{%c{1.}.%M:%L}{INFO=cyan, WARN=cyan, DEBUG=cyan ,FATAL=cyan, ERROR=cyan, TRACE=cyan} %highlight{%level}]: %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>